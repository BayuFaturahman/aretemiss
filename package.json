{
  "name": "i-lead",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 12'",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --simulator='iPhone 8'",
    "ios:small": "ENVFILE=.env.devt react-native run-ios --simulator='iPhone SE (1st generation)'",
    "ios:long": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 11 Pro'",
    "ios:large": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 11'",
    "ios:huge": "ENVFILE=.env.dev react-native run-ios --simulator='iPhone 12 Pro Max'",
    "ios:pod": "pod-install",
    "ios:pod-clean": "cd ios && pod deintegrate && cd ..",
    "xcode": "open ios/iLead.xcworkspace/ -a Xcode.app",
    "android": "ENVFILE=.env.dev react-native run-android",
    "android-prod": "ENVFILE=.env.prod react-native run-android",
    "android:release-debug": "ENVFILE=.env.dev react-native run-android --variant=debug",
    "android:bundle-debug": "cd android && ENVFILE=.env.dev ./gradlew bundleDebug && cd ..",
    "android:build-debug": "cd android && ENVFILE=.env.dev ./gradlew assembleDebug && cd ..",
    "android:release-production": "ENVFILE=.env.prod react-native run-android --variant=release",
    "android:bundle-production": "cd android && ENVFILE=.env.dev ./gradlew bundleRelease && cd ..",
    "android:build-production": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write \"app/**/*.js\"",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx && yarn format",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "generate-image-imports": "node scripts/images.js",
    "generate-svg-imports": "node scripts/svgs.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-clipboard/clipboard": "^1.8.4",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-native-picker/picker": "^2.1.0",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/drawer": "^5.1.1",
    "@react-navigation/native": "~6.0.1",
    "@react-navigation/native-stack": "^6.0.2",
    "@react-navigation/stack": "~6.0.1",
    "accordion-collapse-react-native": "^1.1.0",
    "apisauce": "2.0.0",
    "appcenter": "4.3.0",
    "appcenter-analytics": "4.3.0",
    "appcenter-crashes": "4.3.0",
    "expo-linear-gradient": "^9.2.0",
    "formik": "^2.2.9",
    "i18n-js": "3.8.0",
    "metro-config": "^0.66.2",
    "mobx": "6.1.8",
    "mobx-react-lite": "3.2.0",
    "mobx-state-tree": "5.0.1",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "0.64.4",
    "react-native-actions-sheet": "^0.5.8",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-component": "^0.2.0",
    "react-native-calendar-picker": "^7.1.2",
    "react-native-calendars": "^1.343.0",
    "react-native-config": "^1.4.5",
    "react-native-create-thumbnail": "^1.6.3",
    "react-native-dev-menu": "^4.0.2",
    "react-native-device-info": "^5.6.5",
    "react-native-dropdown-picker": "^5.1.27",
    "react-native-elements": "^2.3.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "1.10.3",
    "react-native-hyperlink": "^0.0.22",
    "react-native-image-picker": "^4.3.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-keychain": "6.2.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal-datetime-picker": "^11.0.0",
    "react-native-modal-selector": "^2.1.0",
    "react-native-modalbox": "^2.0.0",
    "react-native-multi-selectbox": "^1.5.0",
    "react-native-paper": "^4.11.2",
    "react-native-progress": "^4.1.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "3.1.8",
    "react-native-screens": "3.4.0",
    "react-native-size-matters": "^0.3.1",
    "react-native-sms-verifycode": "^2.4.3",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-video-player": "^0.14.0",
    "reactotron-mst": "3.1.3",
    "reactotron-react-js": "^3.3.7",
    "reactotron-react-native": "5.0.0",
    "validate.js": "0.13.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.12.1",
    "@babel/runtime": "^7.12.5",
    "@storybook/addon-storyshots": "6.1.10",
    "@storybook/react-native": "5.3.23",
    "@storybook/react-native-server": "5.3.23",
    "@types/i18n-js": "3.0.3",
    "@types/jest": "26.0.19",
    "@types/react": "16.14.0",
    "@types/react-native": "0.63.40",
    "@types/react-native-video-player": "^0.10.3",
    "@types/react-test-renderer": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "17.14.5",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-native": "^4.0.0",
    "fbjs-scripts": "3.0.0",
    "jest": "^25.5.4",
    "jest-circus": "25.5.4",
    "jest-expo": "^40.0.1",
    "jetifier": "1.6.6",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "^2.5.1",
    "react-devtools-core": "4.10.1",
    "react-dom": "^17.0.2",
    "react-native-clean-project": "^3.6.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "^0.16.3",
    "react-powerplug": "1.0.0",
    "solidarity": "2.3.1",
    "typescript": "4.2.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo-linear-gradient|@react-native|@react-native-async-storage|@react-navigation|@storybook|@react-native-community|expo-localization|@unimodules)"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/iLead.app",
        "build": "xcodebuild -workspace ios/iLead.xcworkspace -scheme iLead -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "name": "iPhone 11"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/iLead.app",
        "build": "xcodebuild -workspace ios/iLead.xcworkspace -scheme iLead -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 11'",
        "type": "ios.simulator",
        "name": "iPhone 11"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "comma-dangle": 0,
      "multiline-ternary": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-use-before-define": "off",
      "quotes": 0,
      "react-native/no-raw-text": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "space-before-function-paren": 0
    }
  }
}
